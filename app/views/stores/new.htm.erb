<%= form_with model: @store, html:{multipart: true} do |form| %>

    <div>
        <%= form.label :name %></br>
        <%= form.text_field :name %>
    </div>
    <div>
        <%= form.label :discription %></br>
        <%= form.text_area :discription %>
    </div>
    <div>
        <%= form.label :phone %></br>
        <%= form.text_field :phone %>
    </div>
    <!--
    <div>
        <%= form.text_field :image %>
    </div>
    -->
    <div>
        <%= form.submit %>
    </div>

<% end %>